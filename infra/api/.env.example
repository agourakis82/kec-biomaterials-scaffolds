########################################
# Darwin API - Example Environment Vars #
########################################

# Auth
API_KEYS=replace-with-secret
API_KEY_REQUIRED=true
NAMESPACE=KEC_BIOMAT_V1

# OpenAI (optional)
OPENAI_API_KEY=sk-xxxx

# Google Cloud / Vertex AI
PROJECT_ID=your-gcp-project
LOCATION=us-central1

# Vertex Vector Search (optional)
VECTOR_INDEX_ID=deployed-index-id
VECTOR_ENDPOINT_ID=index-endpoint-id

# Vertex RAG Engine (optional)
RAG_CORPUS_ID=
VERTEX_EMB_MODEL=text-embedding-004

# RAG++ BigQuery storage (optional)
RAG_PLUS_DATASET_ID=kec_biomat_v1
RAG_PLUS_TABLE_ID=knowledge_base

# Base URL for OpenAPI manifests (.well-known)
BASE_URL=

# Server port (Docker/Cloud Run uses 8080)
PORT=8080
